{
  "title": "Jira configuration",
  "description": "A simple form example.",
  "type": "object",
  "required": [
    "username",
    "token",
    "issue-type",
    "url"
  ],
  "properties": {
    "username": {
      "type": "string",
      "title": "User Name"
    },
    "token": {
      "type": "string",
      "title": "Token"
    },
    "issue-type": {
      "type": "string",
      "title": "Issue Type",
      "default": "Bug"
    },
    "url": {
      "type": "string",
      "title": "Url",
      "format": "uri"
    },
    "multi": {
      "type": "array",
      "items": ["gdfgdg" , "gfdgdfg" , "ggdfgd"]
    }
  }
}
