{
  "title": "Jira configuration",
  "description": "A simple form example.",
  "type": "object",
  "required": [
    "username",
    "token",
    "issue-type",
    "url",
    "phone"
  ],
  "properties": {
    "username": {
      "type": "string",
      "title": "User Name"
    },
    "phone": {
      "type": "string",
      "title": "Phone",
      "format":"phone-us"
    },
    "token": {
      "type": "string",
      "title": "Token"
    },
    "issue-type": {
      "type": "string",
      "title": "Issue Type",
      "default": "Bug"
    },
    "url": {
      "type": "string",
      "title": "Url",
      "format": "uri"
    },
    "urld": {
      "type": "sh",
      "title": "Url"
    }
  }
}
